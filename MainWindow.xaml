<Window x:Class="TalosDownpatcher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="Talos Version Selector"
        Left="50" Top="50" Height="100" Width="300">
  <Grid x:Name="RootGrid">
    <Button HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,0,5,0" Height="24" Width="24" MouseDown="SettingsButton_Click" Click="SettingsButton_Click">
      <Image HorizontalAlignment="Center" VerticalAlignment="Center" Height="24" Width="24" Source="Gear.png"/>
    </Button>
  </Grid>
</Window>
